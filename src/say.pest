WHITESPACE = _{ " " }
sound_name = @{ (ASCII_ALPHANUMERIC | "-" | "_" | "^" | "!" | "~")+ }
speed = @{ "@"? ~ ASCII_DIGIT+ }
pitch = @{ "p" ~ ASCII_DIGIT+ }
wait = @{ "w" ~ ASCII_DIGIT+ }
option = { speed | pitch | wait }
cmd = { sound_name ~ (option)* }
cmds = { SOI ~ cmd ~ (";" ~ cmd)* ~ ";"? ~ EOI }
